{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "product_meta": {
          "type": "product_meta",
          "settings": {
            "show_vendor": true,
            "show_sku": true,
            "show_reviews_badge": false,
            "show_share_buttons": true
          }
        },
        "variant_selector": {
          "type": "variant_selector",
          "settings": {
            "selector_mode": "block",
            "color_mode": "color",
            "show_quantity_selector": true,
            "show_taxes_included": false,
            "show_inventory_quantity": true,
            "low_inventory_threshold": 0
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_payment_button": true
          }
        },
        "description": {
          "type": "description",
          "settings": {
            "display_mode": "show_all"
          }
        },
        "31335ddd-c1ea-40e0-a1af-6a2e064faae3": {
          "type": "text",
          "settings": {
            "content": "<p>{{ product.metafields.sme.release_name.value }}<\/p>"
          }
        },
        "abfdae60-cfb6-47c4-9892-9c6d5db777f5": {
          "type": "text",
          "settings": {
            "content": "<p>Release date: {{ product.metafields.sme.street_date | metafield_tag }}<\/p>"
          }
        },
        "9f8f8e49-899d-4ed8-acc0-5059e6d33878": {
          "type": "text",
          "settings": {
            "content": "<p>Dist. number: {{ product.metafields.sme.distribution_number.value }}<\/p>"
          }
        },
        "3e6f20d1-0dd0-492e-842a-879b7ff34b9b": {
          "type": "text",
          "settings": {
            "content": "<p>FSK: {{ product.metafields.sme.fsk.value }}<\/p>"
          }
        },
        "2e75b2af-c1ad-445e-9263-9bb1a032d880": {
          "type": "text",
          "settings": {
            "content": "<p>Episode: {{ product.metafields.sme.episode.value }}<\/p>"
          }
        }
      },
      "block_order": [
        "product_meta",
        "variant_selector",
        "buy_buttons",
        "description",
        "31335ddd-c1ea-40e0-a1af-6a2e064faae3",
        "abfdae60-cfb6-47c4-9892-9c6d5db777f5",
        "9f8f8e49-899d-4ed8-acc0-5059e6d33878",
        "3e6f20d1-0dd0-492e-842a-879b7ff34b9b",
        "2e75b2af-c1ad-445e-9263-9bb1a032d880"
